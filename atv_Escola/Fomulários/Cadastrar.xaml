<Window x:Class="atv_Escola.Fomulários.Cadastrar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:atv_Escola.Fomulários"
        mc:Ignorable="d"
        Title="Cadastrar" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="80"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="80"/>
        </Grid.RowDefinitions>
        <Label Grid.Row="0" Content="Escola - Cadastro" FontSize="18 px" HorizontalAlignment="Center"/>
        <Label Grid.Row="1" Content="Informações" Margin="20 0"/>
        <Label Grid.Row="1" Content="Nome fantasia"/>
    </Grid>
</Window>
